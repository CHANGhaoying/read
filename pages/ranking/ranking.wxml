<!--pages/ranking/ranking.wxml-->
<navbar />
<view class='ranking'>
  <view class="title">学霸排行榜</view>
  <view class='list'>
    <view class='list-item' wx:key='{{index}}'> 
      <view class='left'>
        <span>3</span>
        <!-- <image src='' class='avatar'></image> -->
        <view class='avatar'>
          <open-data type='userAvatarUrl'></open-data>
        </view>
        <text>傲慢的上校</text>
      </view>
      <view class="right">用时：30秒</view>
    </view>
    <view class='list-item' wx:for='123456789' wx:key='{{index}}'> 
      <view class='left'>
        <span>3</span>
        <!-- <image src='' class='avatar'></image> -->
        <view class='avatar'>
          <open-data type='userAvatarUrl'></open-data>
        </view>
        <text>傲慢的上校十点多是是是</text>
      </view>
      <view class="right">用时：300秒</view>
    </view>
  </view>
</view>
<view class='self'>
  <view class='self-info'>
    <view class='avatar'>
      <open-data type='userAvatarUrl'></open-data>
    </view>
    <view>
      <view class="name"><open-data type='userNickName'></open-data></view>
      <view><text>30秒</text><text>排名:120</text></view>
    </view>
  </view>
  <span>打卡</span>
</view>