/* components/navbar/navbar.wxss */
.top{
  position: relative;
}
.top view{
  height: 32px;
  width:100%;
  padding-left: 10px;
  position: absolute;
  bottom:0;
  left:0;
  line-height: 32px;
  font-size: 16px;
  color: #313131;
}
.top image{
  width:30px;
  height:30px;
  position:absolute;
  left:15px;
  bottom:-3px;
}
.top view text{
  margin-left: 15px;
}
.top view .iconfont{
  display: inline-block;
  font-size: 18px;
  color: #313131;
  font-weight: bold;
  width: 60rpx;
}
@import "../../static/ali/iconfont.wxss"